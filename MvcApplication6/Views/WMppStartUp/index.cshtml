            <div class="col col-lg-6">Vrsta naziva</div>
        </div>
        <div id="Nazivi" data-bind="foreach: data">
            <div class="row noMargin" data-bind="click: function () { $parent.odabrano($index()); }, css: { hasFocus: $parent.odabrano() == $index() }">
