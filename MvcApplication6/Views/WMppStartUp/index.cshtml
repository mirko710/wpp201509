    <script type="text/javascript">

        $(document).ready(function () {

            function swapStyleSheet(sheet) {
                document.getElementById('pagestyle').setAttribute('href', sheet);
            }

            $(window).on("resize", function (event) {
                var wHeight = window.innerHeight;
                var topHeight = $(".page-content-wrapper").children("div:first-child").innerHeight() + $(".row.subHeader").innerHeight() + parseInt($(".row.subHeader").css("marginBottom"));
                //topHeight = 146;
                $(".rightColumn").height(wHeight - topHeight);
                $(".container.glavni").height(wHeight - topHeight);

                var wHeight2 = window.innerHeight;
                var topHeight2 = $(".page-content-wrapper").children("div:first-child").innerHeight() + parseInt($(".page-content-wrapper").children("div:first-child").css("marginBottom"));
                $(".container.glavni.scrollable").height(wHeight2 - topHeight2);
            });

            $('body').on('focusin', '.datarow', function () {
                $(this).addClass("selectedrow");
            });
            $('body').on('focusout', '.datarow', function () {
                $(this).removeClass("selectedrow");
            });



        });
    </script>

